        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:replacer"
        mc:Ignorable="d"
        Title="Special Settings" Height="160.5" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="16*"/>
            <ColumnDefinition Width="83*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Give the  pattern of the document test environment cell pattern" HorizontalAlignment="Left" Height="38" Margin="53,42,0,0" VerticalAlignment="Top" Width="375" Grid.ColumnSpan="2"/>
        <TextBox x:Name="tbPattern" HorizontalAlignment="Left" Height="38" Margin="305,42,0,0" TextWrapping="Wrap" Text="[V] ROBOTTYPE_[NUM]_omniMove | ROBOTTYPE[NUM]" VerticalAlignment="Top" Width="329" Grid.Column="1"/>

    </Grid>
</Window>
